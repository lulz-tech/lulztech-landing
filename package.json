{
  "name": "lulz-tech-landing",
  "description": "Lulz tech landing",
  "version": "0.1.0",
  "private": true,
  "author": "Alex Mubarakshin",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "./node_modules/.bin/eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "./node_modules/.bin/eslint --fix './src/**/*.{ts,tsx}'"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsConfig": "jest.tsconfig.json"
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "enzyme.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/mocks.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/mocks.js"
    }
  },
  "dependencies": {
    "@zeit/next-sass": "1.0.1",
    "@zeit/next-typescript": "1.1.1",
    "isomorphic-unfetch": "3.0.0",
    "next": "latest",
    "next-images": "1.2.0",
    "next-transpile-modules": "2.3.1",
    "node-sass": "7.0.0",
    "postcss": "7.0.24",
    "postcss-preset-env": "6.7.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-ga": "2.7.0",
    "react-scripts-ts": "3.1.0",
    "react-youtube-background": "0.2.1",
    "webpack": "4.41.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.7.4",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.17",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-ga": "2.3.0",
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "babel-loader": "8.0.6",
    "eslint": "6.7.2",
    "eslint-plugin-react": "7.17.0",
    "typescript": "3.7.2"
  }
}
